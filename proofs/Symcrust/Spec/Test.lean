def test_xof : IO Unit := do
  let seed := v!"3456"
  let s := XOF.init
  let s := XOF.absorb s seed

  let (s6, xy) := XOF.squeeze s 1006
  let (s3, x) := XOF.squeeze s 3
  let (s6too, y) := XOF.squeeze s3 1003
  let (_, u) := XOF.squeeze s6 2
  let (_, v) := XOF.squeeze s6too 2
  let z := shake128 seed 1006

  expect "3+3=6" (x ++ y) xy -- associativity
  expect "u=v" u v           -- same-state
  expect "uv=z" xy z       -- same as non-incremental
-/

