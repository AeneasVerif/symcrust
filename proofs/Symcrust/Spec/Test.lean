def test_sha3_256 : IO Unit := do
  let h := v!"a7ffc6f8bf1ed76651c14756a061d662f580ff4de43b49fa82d80a4b80f8434a"
  let h1 ‚Üê time "SHA3_256" Spec.SHA3_256 #[]
  let h1 := bitsToBytes h1
  expect "empty" h h1

  let m := v!""
  let h := v!"4cc561eab9330ace5e39d06721ff75e4b24fff5eb7d146abbbf8e3f9efba6c93"
  let h1 ‚Üê time "SHA3_256" Spec.SHA3_256 (bytesToBits m).toArray
  let h1 := bitsToBytes h1
  expect "273 bytes" h h1

